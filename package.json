{
  "name": "unplugin-convention-routes",
  "type": "module",
  "version": "0.0.1-alpha.3",
  "packageManager": "pnpm@9.15.4",
  "description": "File system base router plugin for unplugin",
  "author": "liujiayii@foxmail.com",
  "license": "MIT",
  "homepage": "https://github.com/liujiayii/unplugin-convention-routes",
  "repository": {
    "type": "git",
    "url": "https://github.com/liujiayii/unplugin-convention-routes"
  },
  "bugs": "https://github.com/liujiayii/unplugin-convention-routes/issues",
  "keywords": [
    "vite",
    "vue",
    "vue-router",
    "react",
    "react-router",
    "solid-js",
    "@solidjs/router",
    "file-system-based",
    "routing"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./farm": {
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "client-react.d.ts",
    "client-solid.d.ts",
    "client-vue.d.ts",
    "dist",
    "virtual-package"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "play": "pnpm --filter vue dev",
    "play:react": "pnpm --filter react dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "publish:ci": "esno scripts/publish.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:fix": "esno scripts/postbuild.ts",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^2.7.0 || ^3.0.0",
    "vite": "^2.0.0 || ^3.0.0-0 || ^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "@solidjs/router": {
      "optional": true
    },
    "@vue/compiler-sfc": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "react-router": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/debug": "^4.1.12",
    "chokidar": "^4.0.3",
    "debug": "^4.4.0",
    "dequal": "^2.0.3",
    "extract-comments": "^1.1.0",
    "fast-glob": "^3.3.3",
    "json5": "^2.2.3",
    "local-pkg": "^1.0.0",
    "picocolors": "^1.1.1",
    "unplugin": "^2.1.2",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/utils": "^8.1.0",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@solidjs/router": "^0.15.3",
    "@types/glob-to-regexp": "^0.4.4",
    "@types/node": "^22.13.0",
    "@vitest/ui": "^3.0.4",
    "@vue/compiler-sfc": "^3.5.13",
    "bumpp": "^10.0.1",
    "chalk": "^5.4.1",
    "eslint": "^9.19.0",
    "esno": "^4.8.0",
    "fast-glob": "^3.3.3",
    "nodemon": "^3.1.9",
    "playwright": "^1.50.1",
    "react": "^19.0.0",
    "react-router": "^7.1.4",
    "rollup": "^4.32.1",
    "solid-js": "^1.9.4",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "webpack": "^5.97.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    }
  }
}
