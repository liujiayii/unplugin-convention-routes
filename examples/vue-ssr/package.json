{
  "name": "vue-ssr",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch ../../dist/*.js -x 'esno server'",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "generate": "DEBUG=unplugin-convention-routes:* vite build --ssrManifest --outDir dist/static && yarn build:server && esno prerender",
    "serve": "NODE_ENV=production esno server"
  },
  "dependencies": {
    "vue": "^3.5.8",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.16.6",
    "@types/serve-static": "^1.15.7",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.5.8",
    "@vue/server-renderer": "^3.5.8",
    "compression": "^1.7.4",
    "esno": "^4.7.0",
    "express": "^4.21.0",
    "fast-glob": "^3.3.2",
    "nodemon": "^3.1.7",
    "serve-static": "^1.16.2",
    "typescript": "^5.6.2",
    "unplugin-convention-routes": "workspace:*",
    "vite": "^5.4.11"
  }
}
